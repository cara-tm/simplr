<txp:if_plugin name="pat_speeder"><txp:pat_speeder /></txp:if_plugin><txp:output_form form="doctype" />
<!-- CSS -->
<txp:css format="link" name="default" />
<!-- ...or you can use (faster) external CSS files e.g. <link rel="stylesheet" href="<txp:page_url type="theme_path" />/styles/default.css"> -->
<style>
code{color:#16a085}

.aside h1 span{margin-right:-.07em}
/* Multiple background for modern browsers */
@supports (-webkit-background-clip:text){.hero{background-image: url(<txp:image_url id="1" link="0" />), url(<txp:php>
$ext = image_info(array('id'=>1,'type'=>'ext'));
echo "images/w_tiny27/1".$ext;
</txp:php>)}
}
picture{min-height:27.4em;filter:grayscale(1)}@media screen and (max-width: 661px){picture{min-height:auto}}

@media print{.wrapper:after{content: "<txp:site_name /> <txp:output_form form="copyright_date" />"}}

</style>
</head>
<body class="default">

<span class="fictiv-font"></span>

<div class="wrapper" id="top">

<txp:output_form form="menu_main" />

	<div class="content">

		<main aria-label="<txp:text item="main_content" />">

			<article class="float-l">

			<section class="float-l">
<txp:if_author>
				<div class="hed mb3">
					<div class="title-bloc">
						<h2 class="title"><txp:text item="articles" /> <txp:text item="by" /> <txp:text item="author" /> <txp:author /><b class="colon">:</b></h2>
					</div>
				</div>
				<txp:article limit="5" sort="Posted desc" />

<txp:else />

				<txp:article limit="5" sort="Posted desc" />

</txp:if_author>
			

			</section>
			</article>

			<txp:output_form form="pages_pager" />

		</main>

	</div> <!-- /content -->
	
	<div class="push"></div>

</div> <!-- /wrapper -->


<txp:output_form form="share" />

<txp:output_form form="footer" />

<txp:output_form form="js" />

</body></html>